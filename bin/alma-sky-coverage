#!/bin/bash
# 
# 
# Example:
#     alma-sky-coverage Coverage_Map_S2COSMOS/Coverage_Polygon_S2COSMOS_interpol.txt
# 

if [[ $(type alma_sky_coverage.py 2>/dev/null | wc -l) -eq 0 ]]; then
    #ls $(dirname "${BASH_SOURCE[0]}")/../SETUP.bash
    source $(dirname "${BASH_SOURCE[0]}")/../SETUP.bash
fi

alma_sky_coverage.py "$@"

